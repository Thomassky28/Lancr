<div class="wrapper">


<p id="notice"><%= notice %></p>


<h1>Services</h1>

  <div class="containerFlex">
    <% @services.each do |service| %>
      <div class="serviceFeed">
        <%= link_to(service) do %>
        <h3><%= service.title %></h3>
        <p><%= service.description %></p>
        <%= image_tag service.img.url %>
        <% end %>
      </div>
    <% end %>
  </div>



<%= link_to 'New Service', new_service_path %>


</div>
